<% city.posts.forEach (p => { %>
    <div class="row">
       <div class="col s12 m6">
         <div class="card blue-grey darken-1">
           <div class="card-content white-text">
             <span class="card-title"><%= p.title %></span>
             <p><%= p.body %></p>
           </div>
           <div class="card-action">
                   <a href="#">Edit</a>
                   <a href="#">Delete</a>
                 </div>
         </div>
       </div>
       <% }) %>
     </div>